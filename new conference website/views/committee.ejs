    <script>
        var view = document.getElementById("menu_list");

        var burger_icon_open = document.getElementById("burger_icon_open");

        var burger_icon_close = document.getElementById("burger_icon_close");

        burger_icon_open.addEventListener('click', function () {

            view.style.display = "block";
            burger_icon_open.style.display = "none";
            burger_icon_close.style.display = "block";


        });

        burger_icon_close.addEventListener('click', function () {

            view.style.display = "none";
            burger_icon_open.style.display = "block";
            burger_icon_close.style.display = "none";
        });

        // committee
        var advisory_btn = document.getElementById("advisory-btn");
        var volunteer_btn = document.getElementById("volunteer-btn");
        var technical_btn = document.getElementById("technical-btn");
        var organising_btn = document.getElementById("organising-btn");

        var advisory_list = document.getElementById("advisory-list");
        var volunteer_list = document.getElementById("volunteer-list");
        var technical_list = document.getElementById("technical-list");
        var organising_list = document.getElementById("organising-list");

        advisory_btn.addEventListener('click', function(){
            advisory_list.style.display = "block";
            volunteer_list.style.display = "none";
            technical_list.style.display = "none";
            organising_list.style.display = "none";
        });
        volunteer_btn.addEventListener('click', function(){
            advisory_list.style.display = "none";
            volunteer_list.style.display = "block";
            technical_list.style.display = "none";
            organising_list.style.display = "none";
        });
        technical_btn.addEventListener('click', function(){
            advisory_list.style.display = "none";
            volunteer_list.style.display = "none";
            technical_list.style.display = "block";
            organising_list.style.display = "none";
        });
        organising_btn.addEventListener('click', function(){
            advisory_list.style.display = "none";
            volunteer_list.style.display = "none";
            technical_list.style.display = "none";
            organising_list.style.display = "block";
        });

    </script>
