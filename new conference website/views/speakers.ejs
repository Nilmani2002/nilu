    <script>

        var view = document.getElementById("menu_list");

        var burger_icon_open = document.getElementById("burger_icon_open");

        var burger_icon_close = document.getElementById("burger_icon_close");

        burger_icon_open.addEventListener('click', function () {

            view.style.display = "block";
            burger_icon_open.style.display = "none";
            burger_icon_close.style.display = "block";


        });

        burger_icon_close.addEventListener('click', function () {

            view.style.display = "none";
            burger_icon_open.style.display = "block";
            burger_icon_close.style.display = "none";
        });


        var slide_index1 = 1;
        displaySlides1(slide_index1);




        function nextSlide1(n) {
            displaySlides1(slide_index1 += n);
        }

        function currentSlide1(n) {
            displaySlides1(slide_index1 = n);
        }

        function displaySlides1(n) {
            var j;
            var slides1 = document.getElementsByClassName("showSlide1");
            if (n > slides1.length) { slide_index1 = 1 }
            if (n < 1) { slide_index1 = slides1.length }
            for (j = 0; j < slides1.length; j++) {
                slides1[j].style.display = "none";
            }
            slides1[slide_index1 - 1].style.display = "block";
        }

        setInterval(function () {
            nextSlide1(1);
        }, 3000);

        // session speaker section--------------------------------------
        var slide_index2 = 1;
        displaySlides2(slide_index2);




        function nextSlide2(o) {
            displaySlides2(slide_index2 += o);
        }

        function currentSlide2(o) {
            displaySlides2(slide_index2 = o);
        }

        function displaySlides2(o) {
            var k;
            var slides2 = document.getElementsByClassName("showSlide2");
            if (o > slides2.length) { slide_index2 = 1 }
            if (o < 1) { slide_index2 = slides2.length }
            for (k = 0; k < slides2.length; k++) {
                slides2[k].style.display = "none";
            }
            slides2[slide_index2 - 1].style.display = "block";
        }

        setInterval(function () {
            nextSlide2(1);
        }, 3000);

        // Response of clicking on keynote speakers and session speakers button------
        var keynote_speakerGallery = document.getElementById("keynote_speakerGallery");
        var keynote_btn = document.getElementById("keynote_btn");

        keynote_btn.addEventListener('click', function () {

            keynote_speakerGallery.style.display = "block";
            session_speakerGallery.style.display = "none";
            keynote_btn.style.opacity = "100%";
            session_btn.style.opacity = "65%";
        });

        var session_speakerGallery = document.getElementById("session_speakerGallery");
        var session_btn = document.getElementById("session_btn");

        session_btn.addEventListener('click', function () {
